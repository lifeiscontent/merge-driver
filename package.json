{
  "name": "@pnpm/merge-driver",
  "version": "0.1.0",
  "description": "A merge driver for shrinkwrap.yaml",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "bin": {
    "pnpm-merge-driver": "lib/cli.js"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "tslint -c tslint.json --project .",
    "tsc": "rimraf lib && tsc",
    "test": "rimraf .store && npm run lint && preview && jest",
    "md": "mos",
    "prepublishOnly": "npm run tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pnpm/merge-driver.git"
  },
  "keywords": [
    "pnpm",
    "git",
    "merge",
    "driver"
  ],
  "author": "Zoltan Kochan <z@kochan.io> (https://www.kochan.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/merge-driver/issues"
  },
  "homepage": "https://github.com/pnpm/merge-driver#readme",
  "dependencies": {
    "@pnpm/lockfile-file": "^3.0.15",
    "@pnpm/logger": "^3.0.0",
    "@pnpm/prune-lockfile": "^2.0.15",
    "@types/ramda": "^0.27.0",
    "ramda": "^0.27.0"
  },
  "devDependencies": {
    "@types/tape": "^4.2.31",
    "jest": "^26.6.1",
    "package-preview": "^3.0.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^26.4.3",
    "tslint": "^6.0.0",
    "typescript": "^4.0.0"
  },
  "mos": {
    "plugins": [
      "readme"
    ],
    "installation": {
      "useShortAlias": true
    }
  }
}
